{
  "$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
  "diagnostics": {
    "disable": [
      "access-invisible",
      "unnecessary-if",
      "redefined-local"
    ]
  },
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "ignoreDir": [
      ".prefix"
    ],
    "library": [
      "$VIMRUNTIME",
      "library/busted",
      "library/luv"
    ]
  }
}
